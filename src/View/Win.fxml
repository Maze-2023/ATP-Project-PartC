<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.media.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.WinController">
    <MediaView fx:id="mediaView" fitHeight="400.0" fitWidth="600.0" />

    <HBox alignment="CENTER" layoutX="0" layoutY="320">
        <Button onAction="#exit" mnemonicParsing="false" text="Quit" />
        <Button onAction="#start" mnemonicParsing="false" text="New game" />
    </HBox>
</AnchorPane>
